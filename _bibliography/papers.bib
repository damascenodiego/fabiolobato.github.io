@inproceedings{
 title = {An Evolutionary Missing Data Imputation Method for Pattern Classification},
 type = {inproceedings},
 year = {2015},
 keywords = {data imputation,evolutionary computing,genetic algorithms,missing data},
 pages = {1013-1019},
 websites = {http://doi.acm.org/10.1145/2739482.2768451},
 publisher = {ACM},
 city = {New York, NY, USA},
 series = {GECCO Companion '15},
 id = {f1bf2438-a1b9-3504-b6a9-0a7b9c6eca65},
 created = {2020-04-15T19:49:23.569Z},
 file_attached = {false},
 profile_id = {bd1f7f2e-7152-3c06-841d-eee1e29b03d2},
 group_id = {0ea50baf-be5a-3ec5-9548-2232bf737587},
 last_modified = {2020-04-15T19:49:23.694Z},
 read = {false},
 starred = {false},
 authored = {false},
 confirmed = {false},
 hidden = {false},
 source_type = {inproceedings},
 private_publication = {false},
 abstract = {Data analysis plays an important role in our Information Era; however, most of statistical and machine learning algorithms were not developed to tackle the ubiquitous issue of missing values. In pattern classification, several strategies have been proposed to handle this problem, where missing data imputation is the most used one, which can be viewed as an optimization problem where the goal is to reduce the bias imposed by the absence of information. Although most imputation methods are restricted to one type of variable only (categorical or numerical), they usually ignore information within incomplete instances. To fill these gaps, we propose an evolutionary missing data imputation method for pattern classification, based on a genetic algorithm, which is suitable for mixed-attribute datasets and takes into account information from incomplete instances and model building - more specifically, the classification accuracy. To assess the performance of our method, we used three algorithms in order to represent the three groups of classification methods: 1) rule induction learning, 2) approximate models and 3) lazy learning. Experiments have shown that the proposed method outperforms some well-established missing value treatment methods. Copyright is held by the owner/author(s).},
 bibtype = {inproceedings},
 author = {Lobato, Fábio M F and Tadaiesky, Vincent W and Araújo, Igor M and de Santana, Ádamo L},
 doi = {10.1145/2739482.2768451},
 booktitle = {Proceedings of the Companion Publication of the 2015 on Genetic and Evolutionary Computation Conference}
}

