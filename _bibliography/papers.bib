@article{
 title = {Deep learning in gastric tissue diseases: a systematic review},
 type = {article},
 year = {2020},
 pages = {e000371},
 volume = {7},
 websites = {http://bmjopengastro.bmj.com/lookup/doi/10.1136/bmjgast-2019-000371},
 month = {3},
 publisher = {BMJ Specialist Journals},
 day = {26},
 id = {d0b98101-d90e-3643-b3a5-fc66cba2ad0c},
 created = {2020-04-17T01:36:34.213Z},
